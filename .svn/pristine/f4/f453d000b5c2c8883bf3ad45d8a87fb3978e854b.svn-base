.ableDrag .ivu-menu-item{
		position: relative;
		min-height: 320px;
	}
	.ableDrag .ivu-menu-item .tijiaoForm, .ableDrag .ivu-menu-item .shenpiForm,{
		position: absolute;
		top: 0;
		left: 0;
		width: 100%;
		display: none;
	}
	.ableDrag .ivu-menu-item .tijiaoForm:first-child,.ableDrag .ivu-menu-item .shenpiForm:first-child{
		display: block;
	}
	.ableDrag .ivu-menu-item .tijiaoForm:first-child .formitem,.ableDrag .ivu-menu-item .shenpiForm:first-child .formitem{
		display: none;
	}
	.ableDrag .ivu-menu-item .tijiaoForm:first-child .formitem:first-child,.ableDrag .ivu-menu-item .shenpiForm:first-child .formitem:first-child{
		display: block;
	}
	.ableDrag .dragInputForm{
		position: relative;
		display: none;
	}
	.ableDrag .dragInputForm .dragInput{
		position: absolute;
		top: 0;
		left: 0;
		width: 100%;
		
	}
	.ableDrag .dragInputForm:first-child{
		display: block;
	}
	.ableDrag .moni{
		cursor: move;
	}
	.moni{
		width: 100px;
		height: 34px;
		/*margin: 0 auto;*/
		line-height: 34px;
		border: 1px solid #ddd;
		background: #0C8DA0;
		border-radius: 4px;
		text-align: center;
		font-size: 12px;
		font-family: "微软雅黑";
		color: #fff;
		position: relative;
	}
	.ableDrag .anniu{
		position: absolute;
		top: 0;
		left: 0;
	}
	.dropArea .moni{
		margin: 10px 0;
	}
	.dropArea .moni .setThis{
		position: absolute;
		/*width: 80px;*/
		left: 130px;
		display: inline-block;
		text-align: center;
		cursor: pointer;
		border: none;
	}
	.ableDrag .delthis,.ableDrag .setThis{
		display: none;
	}
	.ableDrag .colGrid{
		width: 90%;
		height: 30px;
		min-height: 20px;
		cursor: move;
		position: absolute;
		top: 0;
		left: 0;
	}
	.ableDrag .upload{
		position: absolute;
		width: 100%;
		top: 0;
		left: 0;
	}
	.dropArea .ivu-col,.ableDrag .ivu-col,.dropInputArea .ivu-col{
		min-height: 40px;
		padding: 10px 0;
		border: 1px dashed #ddd;
	}
	.dropInputArea .dragInput .ivu-col,.dragInputCss .dragInput .ivu-col{
		border: none;
	}
	.close{
		position: absolute;
		
	}
	.ableDrag .elcss{
		position: absolute;
		width: 100%;
		height: 270px;
		top: 0;
		left: 0;
		right: 0;
		bottom: 0;
		margin: auto;
	}
	#uploadfiledrag>.elcss,#onlyuploaddrag>.elcss,#onlyfilesdrag>.elcss{
		height: 100px;
		line-height: 100px;
	}
	#uploadfiledrag .elcss .setting,#onlyuploaddrag .elcss .setting,#onlyfilesdrag .elcss .setting{
		width: 100%;
		height: 100px;
		line-height: 100px;
		position: relative;
	}
	#uploadfiledrag .elcss .setting img,#onlyuploaddrag .elcss .setting img,#onlyfilesdrag .elcss .setting img{
		width: 60%;
		max-height: 60px;
		position: absolute;
		top: 0;
		left: 0;
		right: 0;
		bottom: 0;
		margin: auto;
	}
	.myLabel,.myList{
		height: 30px;
		padding: 20px 0;
		line-height: 30px;
		font-size: 16px;
		padding-left: 10px;
	}
	.ableDrag .myCard{
		height: 160px;
	}
	.myList{
		height: 130px;
	}
	.ableDrag .mystrongList{
		height: 130px;
	}
	.ableDrag .mystrongList .searchbox,.ableDrag .mystrongList .delLabel,.ableDrag .mystrongList .setDummy{
		display: none;	
	}
	.ableDrag .myLabel,.ableDrag .myList{
		width: 100%;
		height: 30px;
		padding: 0;
		padding-left: 40px;
		cursor: move;
	}
	.elcss .setting{
		width: 100%;
		height: 100%;
		position: relative;
		background: transparent;
		border: none;
	}
	.ableDrag .elcss .setting .delChart,.ableDrag .elcss .setting .delLabel{
		display: none;
	}
	.ableDrag .elcss .setting .setDummy{
		display: none;
	}
	.dummyUI .setting .delChart,.pieChart .setting .delChart,.myCard .setting .delChart{
		position: absolute;
		width: 20px;
		height: 20px;
		line-height: 20px;
		right: 60px;
		top: 0px;
	}
	.myLabel .setting .delLabel,.myList .setting .delLabel,.myCard .setting .delLabel{
		position: absolute;
		/*width: 60px;*/
		height: 30px;
		top: -5px;
		left: 160px;
	}
	.elcss .setting .setDummy{
		position: absolute;
		/*width: 60px;*/
		height: 30px;
		left: 10%;
		top: -10px;
	}
	.myLabel .setting .setDummy,.myList .setting .setDummy{
		top: -5px;
	}
	.dropInputArea .colGrid,.dropArea .colGrid{
		position: relative;
		margin: 10px;
		margin-right: 60px;
	}
	.dropInputArea .colGrid .delthis,.dropArea .colGrid .delthis{
		position: absolute;
		top: 0;
		bottom: 0;
		right: -40px;
		margin: auto;
	}
	.dropArea .moni .delthis{
		position: absolute;
		width: 24px;
		height: 24px;
		line-height: 24px;
		text-align: center;
		border-radius: 50%;
		background: #FF0000;
		top: -12px;
		right: -12px;
		font-size: 10px;
		cursor: pointer;
	}
	.mystrongList.elcss .setting .setDummy{
		position: absolute;
		top: auto;
		bottom: 0;
		height: auto;
		margin-left: 10px;
	}
	.dropArea .myList.elcss{
		height: 220px;
	}
	.myList.elcss .setting .setDummy{
		left: 0;
		bottom: 0;
		top: auto;
		height: auto;
	}
	.myList.elcss .setting .delLabel{
		position: absolute;
		height: auto;
		bottom: 0;
		top: auto;
	}
	.dropArea .myLabel.elcss .setting .setDummy,.dropArea .myLabel.elcss .setting .delLabel{
		position: static;
		display: inline-block;
		margin-left: 20px;
	}
	#qleditordrag .qleditor,#Approvestepdrag .Approvestep,#Approvehistorydrag .Approvehistory,#messagecarddrag .messagecard{
		height: auto;
	}
	/*可拖拽区域为fixed*/
	.dragfixed{
		position: fixed;
		width: 16%;
		height: auto;
		left: 24%;
		top: 10%;
	}
	.dropArea .elcss{
		margin: 30px 0;
	}
	img{
		max-width: 100%;
	}